
@model IEnumerable<mvcapp.Models.Employee>
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>


<table class="table table-striped">
    @{
        foreach (var i in Model)
    {
        <tr>
        <td>@i.Id</td>
        <td>@i.Name</td>
        <td>@i.Salary</td>
        <td>@i.JoinDate</td>
        </tr>
    }
    }
</table>