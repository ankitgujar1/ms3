@model IEnumerable<mock2.Models.BloodDonor>

<table class="table table-striped">
    <tr>
        <th>@Html.DisplayNameFor(model=>model.Name)</th>
        <th>@Html.DisplayNameFor(model=>model.DOB)</th>
        <th>@Html.DisplayNameFor(model=>model.Gender)</th>
        <th>@Html.DisplayNameFor(model=>model.BloodGroup)</th>
        <th>@Html.DisplayNameFor(model=>model.MobileNumber)</th>
        <th>@Html.DisplayNameFor(model=>model.Email)</th>
        <th>@Html.DisplayNameFor(model=>model.Location)</th>
        <th>Actions</th>
    </tr>

    @foreach (var i in Model)
    {
        <tr>
            <td>@i.Name</td>
            <td>@i.DOB</td>
            <td>@i.Gender</td>
            <td>@i.BloodGroup</td>
            <td>@i.MobileNumber</td>
            <td>@i.Email</td>
            <td>@i.Location</td>
            <td><a asp-action="Delete" asp-route-id=@i.ID>Delete</a></td>
            <td><a asp-action="Edit" asp-route-id=@i.ID>Edit</a></td>
        </tr>

    }
</table>